Section:NewButton("Auto Name Removal", "Removes Your Username", function()
        print("Clicked")
local P = game.Players.LocalPlayer
while wait() do
if P.Character and P.Character.Head.Username then
P.Character.Head.Username:Destroy()
end

end)
